<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sulu_route.route_manager.class">Sulu\Bundle\RouteBundle\Manager\RouteManager</parameter>
        <parameter key="sulu_route.route_repository.class">Sulu\Bundle\RouteBundle\Entity\RouteRepository</parameter>
    </parameters>

    <services>
        <service id="sulu_route.route_manager" class="%sulu_route.route_manager.class%">
            <argument>%sulu_route.route_repository.class%</argument>
        </service>
    </services>
</container>